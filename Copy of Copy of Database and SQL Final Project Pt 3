RENCES Monsters(mID),\n","  title VARCHAR(50),\n","  duration INTEGER,\n","  CONSTRAINT duration_ck CHECK (duration BETWEEN 30 AND 180),\n","  meeting VARCHAR(7),\n","  starting_time VARCHAR(7),\n","  size INTEGER\n",");"],"metadata":{"id":"rQ9CiBEREYCC","colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"status":"ok","timestamp":1651949040839,"user_tz":300,"elapsed":167,"user":{"displayName":"Indom","userId":"11143856035438375440"}},"outputId":"98f73cc1-31ba-4f62-b2de-9f3cc0575e78"},"execution_count":71,"outputs":[{"output_type":"stream","name":"stdout","text":[" * postgresql://postgres:***@localhost/my_data\n","Done.\n"]},{"output_type":"execute_result","data":{"text/plain":["[]"]},"metadata":{},"execution_count":71}]},{"cell_type":"code","source":["%%sql \n","\n","CREATE TABLE Locations(\n","  loID INTEGER PRIMARY KEY NOT NULL,\n","  code VARCHAR(2),\n","  room INTEGER,\n","  CONSTRAINT room_ck CHECK (room BETWEEN 1 AND 2000),\n","  capacity INTEGER,\n","  CONSTRAINT capacity_ck CHECK (capacity BETWEEN 10 AND 300)\n",");"],"metadata":{"id":"wuFP3p1Qissu","colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"status":"ok","timestamp":1651949040005,"user_tz":300,"elapsed":145,"user":{"displayName":"Indom","userId":"11143856035438375440"}},"outputId":"42deead1-84a1-42d8-9695-2b6ef164a8b2"},"execution_count":70,"outputs":[{"output_type":"stream","name":"stdout","text":[" * postgresql://postgres:***@localhost/my_data\n","Done.\n"]},{"output_type":"execute_result","data":{"text/plain":["[]"]},"metadata":{},"execution_count":70}]},{"cell_type":"code","source":["%%sql \n","\n","CREATE TABLE Monsters2Classes(\n","  monsters_mID INTEGER NOT NULL,\n","  FOREIGN KEY(monsters_mID) REFERENCES Monsters(mID),\n","  classes_cID INTEGER NOT NULL,\n","  FOREIGN KEY(classes_cID) REFERENCES Classes(cID)\n",");"],"metadata":{"id":"RYOtxGSSQwxb","colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"status":"ok","timestamp":1651949041972,"user_tz":300,"